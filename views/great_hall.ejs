<div class="GH_room">
    <h3>Great Hall</h3>
        <% if(charData.length != 0) { %>
            <table class="striped">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Status</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <% charData.forEach( el => { %>
                    <tr>
                        <td>
                            <a href="./houses/person/<%=el._id%>">
                            <%= el.name %>
                            </a>
                        </td>
                        <td>
                            <span class="dash-status">
                            <%= el.status %>
                            </span>
                        </td>
                    </tr>
                <% }) %>
            </tbody>
            </table>
        <% } else { %>
        <p>You have not added any Characters.</p>
    <% } %>
</div>