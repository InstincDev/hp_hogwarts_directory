<div class="row">
    <div class="col s12 m8">
        <h3>
            <%= person.name %>
        </h3>
        <div class="card story">
            <div class="card-content">
                <table class="striped">
                    <tbody>
                        <% for(let el in person ) { %>
                            <tr>
                                <% if(el != "_id" && el != "image" && el != "__v"){%> 
                                <th>
                                    <%= el %>:
                                </th>
                                <td>
                                    <%= person[el] %>      
                                </td>
                                <% } %>  
                            </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="col s12 m4">
        <div class="card center-align">
            <div class="card-content">
                <!-- Change: add 'referrerpolicy="no-referrer"' to stop google from blocking access to the profile images -->
                <img src="<%= person.image %>" class="circle responsive-img img-small">
            </div>
        </div>
    </div>
</div>